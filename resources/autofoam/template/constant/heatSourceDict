FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      heatSourceProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

sources (beam);

beam
{
    pathName            scanPath;    

    absorptionModel     Kelly;    
    KellyCoeffs
    {
        geometry        cylinder;
        eta0            0.3;
        etaMin          0.35;
    }   
    
    heatSourceModel     modifiedSuperGaussian;
    modifiedSuperGaussianCoeffs
    {
        dimensions      (50.0e-6 50.0e-6 30e-6);
        m               2.0;
        k               2.0;
        transient       true;
        isoValue        1670;
    }
}
