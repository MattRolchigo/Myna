FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    object          blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

xmin            0.042499789999999996;
ymin            -0.05550861;
zmin            -2.01e-06;

xmax            0.05749781;
ymax            -0.041172589999999995;
zmax            0.016500010000000002;

nx              75;
ny              72;
nz              83;

vertices
(
    ( $xmin $ymin $zmin )
    ( $xmax $ymin $zmin )
    ( $xmax $ymax $zmin )
    ( $xmin $ymax $zmin )
    ( $xmin $ymin $zmax ) 
    ( $xmax $ymin $zmax )
    ( $xmax $ymax $zmax ) 
    ( $xmin $ymax $zmax ) 
);

blocks
(
    hex ( 0 1 2 3 4 5 6 7 ) ( $nx $ny $nz ) simpleGrading ( 1 1 1 )
);

edges( );

boundary
(
    part
    {
        type wall;
        faces
        (
            ( 0 3 2 1 ) 
            ( 4 5 6 7 )
            ( 0 4 7 3 )
            ( 2 6 5 1 )
            ( 1 5 4 0 )
            ( 3 7 6 2 )
        ); 
    }
);

mergePatchPairs ( );

